CREATE TABLE movie (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT,
    backdropImage TEXT,
    posterImage TEXT,
    updated_at TEXT DEFAULT(STRFTIME('%Y-%m-%d %H:%M:%f', 'NOW')) NOT NULL
);

getMovies:
SELECT *
FROM movie;

getMovie:
SELECT *
FROM movie
WHERE id == ?;

insertMovie:
REPLACE INTO movie (id, title, backdropImage, posterImage)
VALUES (?,?,?,?);

clearMovies:
DELETE FROM movie;

